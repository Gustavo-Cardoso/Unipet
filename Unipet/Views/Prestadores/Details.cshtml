@model Unipet.Models.Prestador

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="app-wrapper">
    <div class="app-content pt-3 p-md-3 p-lg-4">
        <div class="container-xl">
            <div class="app-card alert alert-dismissible shadow-sm mb-4" role="alert">
                <div class="inner">
                    <div class="app-card-body p-3 p-lg-4">
                        <a asp-action="Index" style="padding-left: 20px; float: right"> <i class="fas fa-reply"></i> Voltar</a>
                        <h4 class="mb-3">Detalhes do prestador</h4>
                        <div class="row gx-5 gy-3">
                            <div class="col-12 col-lg-12">
                                <div class="col-md-12">
                                    <dl class="row mt-3 mb-5">
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.RazaoSocial)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.RazaoSocial)
                                        </dd>
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.CNPJ)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.CNPJ)
                                        </dd>
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Fone)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Fone)
                                        </dd>
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Email)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Email)
                                        </dd>
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Endereco)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Endereco.Logradouro),
                                            @Html.DisplayFor(model => model.Endereco.NumeroCasa) <br />
                                            Bairro: @Html.DisplayFor(model => model.Endereco.Bairro) .
                                            CEP: @Html.DisplayFor(model => model.Endereco.CEP)
                                        </dd>
                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Usuario)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Usuario.Id)
                                        </dd>

                                        <dt class="col-sm-2">
                                            @Html.DisplayNameFor(model => model.Logotipo)
                                        </dt>
                                        <dd class="col-sm-10">
                                            @Html.DisplayFor(model => model.Logotipo)
                                        </dd>
                                        <dt class="col-sm-1 mt-3">
                                            @Html.DisplayNameFor(model => model.ExibirHome)
                                        </dt>
                                        <dd class="col-sm-10 mt-3">
                                            @Html.DisplayFor(model => model.ExibirHome)
                                        </dd>
                                    </dl>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div>